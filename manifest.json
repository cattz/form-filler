
{
  "manifest_version": 3,
  "name": "Form Autofiller Assistant",
  "version": "1.1",
  "description": "Autorrellena formularios desde un archivo Excel con detección automática de campos.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}