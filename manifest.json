{
  "manifest_version": 3,
  "name": "Form Autofiller Assistant (Full v3)",
  "version": "1.3",
  "description": "Autorrellena formularios desde CSV con mapeos espec√≠ficos y por defecto.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}